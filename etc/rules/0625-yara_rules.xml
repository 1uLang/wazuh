<group name="yara,gdpr_IV_35.7.d">

  <rule id="65100" level="0">
    <category>ossec</category>
    <decoded_as>yara</decoded_as>
    <options>no_full_log</options>
    <description>Group of YARA rules</description>
  </rule>

  <rule id="65101" level="6">
    <if_sid>65100</if_sid>
    <field name="yara.type">^file</field>
    <options>no_full_log</options>
