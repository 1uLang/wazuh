  <!-- System inventory -->
  <inventory>
    <enabled>yes</enabled>
    <interval>1h</interval>
    <scan_on_start>yes</scan_on_start>
    <hardware>no</hardware>
    <os>yes</os>
    <network>no</network>
    <packages>no</packages>
    <ports all="no">no</ports>
    <processes>no</processes>
  </inventory>
 
