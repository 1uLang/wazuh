<decoder name="process_list">
  <parent>ossec</parent>
  <use_own_name>true</use_own_name>
  <regex offset="after_parent">'process list':\s+(\d+)\s+(\w+)\s+(\S+\.*)$</regex>
  <order>pid, user, command</order>
</decoder>
